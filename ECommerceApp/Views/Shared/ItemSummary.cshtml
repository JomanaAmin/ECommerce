@model Item

    <div class="menu-item shadow rounded col-md-3 col-6">
        <div>
            <div>
                <img class="img-fluid" src="@Model.imageUrl" alt="@Model.Name" width="100" />
                <p class="lead">@Model.Name <p>
                <p class="lead">@Model.Description </p>
                <p class="lead">@Model.Price </p>
                <p class="lead">@Model.Category.Name </p>
                @* <p class="lead">Availability: @(Model.IsAvailable ? "Available" : "Not Available") </p> *@
            </div>
        </div>

    </div>
